<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox prefHeight="800.0" prefWidth="1000.0" spacing="10.0" style="-fx-background-color: #282828;"
      xmlns="http://javafx.com/javafx/21.0.0" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.simplewikibrowser.SimpleWikiBrowserController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <TextField fx:id="searchField" promptText="Enter search term here" />
    <Button text="Search" onAction="#handleSearch" />
    <WebView fx:id="webView" VBox.vgrow="ALWAYS" />
</VBox>
